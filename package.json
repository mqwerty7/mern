{
  "name": "mern-course",
  "version": "1.0.0",
  "description": "Mern stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENW=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENW=development concurrently \"npm run server\" \"npm run client\" "
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Panasevich Evgeniy",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "http-proxy-middleware": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2"
  }
}
